<data>
  <xpath expr="/form/field[@name='company']" position="after">
    <group col="3" colspan="4" id="buttons" String="Tracker">
      <button name="start_work_wizard" string="Start Tracker"/>
      <button name="stop_work" string="Stop Tracker"/>
      <button name="cancel_work" string="Cancel Tracker"/>
    </group>
  </xpath>
  <xpath expr="/form/notebook" position="inside">
      <page name="working_employees">
        <field name="working_employees" colspan="4"/>
      </page>

  </xpath>
</data>
